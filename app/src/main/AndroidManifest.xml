<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yxy.yxydemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/BaseTheme">
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TestOneActivity" />
        <activity android:name=".TestTwoActivity"/>
        <activity android:name=".activity.TestThreeActivity" />
        <activity android:name=".activity.TestFourActivity"
            android:windowSoftInputMode="adjustResize|stateHidden"/>
    </application>

</manifest>